{
  "name": "svg2react",
  "version": "0.4.1",
  "description": "CLI to convert SVG to React component",
  "main": "lib/svg-to-react.js",
  "bin": {
    "svg2react": "./lib/index.js"
  },
  "scripts": {
    "clean": "cross-env --c-only-linux rm -rf lib",
    "build": "npm run clean && babel src --out-dir lib && cross-env --c-only-linux chmod +x lib/index.js",
    "start": "babel-node src",
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meriadec/svg-to-react.git"
  },
  "keywords": [
    "svg",
    "react",
    "convert"
  ],
  "author": "Meriadec Pillet",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/meriadec/svg-to-react/issues"
  },
  "homepage": "https://github.com/meriadec/svg-to-react#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "commander": "^2.9.0",
    "cross-env": "mr47/cross-env",
    "glob": "^7.1.2",
    "lodash": "^4.17.4",
    "svgo": "^0.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.19.0",
    "eslint-config-zavatta": "^4.2.0",
    "expect": "^1.20.2",
    "mocha": "^3.2.0"
  }
}
